{
  "name": "cesnet-kafka",
  "version": "1.1.0",
  "author": "cesnet",
  "summary": "Apache Kafka Puppet Module",
  "license": "MIT",
  "source": "https://github.com/MetaCenterCloudPuppet/cesnet-kafka",
  "project_page": "https://github.com/MetaCenterCloudPuppet/cesnet-kafka",
  "issues_url": "https://github.com/MetaCenterCloudPuppet/cesnet-kafka/issues",
  "tags": [
    "kafka",
    "messaging"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "16.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 2.7.0"
    }
  ],
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 2.0.0"},
    {"name":"cesnet/hadoop_lib","version_requirement":">= 0.3.0 <1.0.0"},
    {"name":"cesnet/zookeeper","version_requirement":"<2.0.0"}
  ]
}
